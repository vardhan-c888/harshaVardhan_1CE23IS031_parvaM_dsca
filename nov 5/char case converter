#include<stdio.h>
int main()
{
    char ch;
    printF("enter a character:");
    scanf("%d",ch);
    if (ch>='a' &&ch<='z'){
        ch=upper(ch);
        printf("converted to uppercase:%c\n",ch);
    }
     elseif (ch>='A' &&ch<='Z'){
        ch=lower(ch);
        printf("converted to lowercase:%c\n",ch);
    else{
        printf("the input is not an alphabtic character.\n");
    }
     }
     return 0;
}